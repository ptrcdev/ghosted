{
  "version": 2,
  "buildCommand": "npm run vercel-build --workspace=@job-tracker/backend",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "apps/backend/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/backend/api/index.ts"
    }
  ]
}